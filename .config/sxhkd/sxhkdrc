#!/usr/bin/sxhkd
super + shift + q
               bspc quit

{super,super + shift} + {1,2,3,4,5}
                    bspc {desktop --focus,node -d}  {web,term,chat,code,music}

super + {_,shift + }{h,j,k,l}
    bspc node -{f,s} {west,south,north,east}

Print
    flameshot gui -p ~/Pictures/screenshots

super + {t,shift + t,s,f}
    bspc node -t {tiled,pseudo_tiled,floating,fullscreen}

super + ctrl + {m,x,y,z}
    bspc node -g {marked,locked,sticky,private}

super + r
    ~/.config/bspwm/bspwmrc && pkill -USR1 -x sxhkd

super + Return
               alacritty -e bash -c "(tmux ls | grep -qEv 'attached|scratch' && tmux at) || tmux"

super + shift + Return
               alacritty

super + shift + e
                emacs

super + e
                emacsclient -c

super + @space
                vimb searx.neocities.org

#!/usr/bin/sxhkd
super + d
               rofi -show run

super + a
               rofi -show ssh

super + x
               rofi-keepassxc -d ~/Daten/cloud/tlaloc/sec/db.kdbx

super + {_,shift +}w
                bspc node -{c,k}
