Xcursor.theme: gentoo-blue
Xcursor.size: 32

Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight


*background: #282828
*foreground: #fbf1c7
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

! black
*.color0:       #292f34
*.color8:       #0a6355

! red
*.color1:       #ab4642
*.color9:       #ab4642

! green
*.color2:       #34BE5B
*.color10:      #34BE5B

! yellow
*.color3:       #f7ca88
*.color11:      #f7ca88

! blue
*.color4:       #006a78
*.color12:      #006a78

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #d8d8d8
*.color15:      #f8f8f8


urxvt*geometry:             90x30
urxvt*loginShell:           true
urxvt*internalBorder:       0
urxvt*lineSpace:            0

URxvt*saveLines: 12000
urxvt*depth: 32

urxvt*font: xft:Monoid:pixelsize=13
URxvt*letterSpace: -1

URxvt*scrollBar: false

URxvt*scrollstyle: plain
